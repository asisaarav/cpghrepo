			},
			"segment_grp": {
				"type": "string",
				"defaultValue": "customer"
			},
			"file_name": {
				"type": "string"
			},
			"brz_nb_path": {
				"type": "string"
			},
			"slv_nb_path": {
				"type": "string"
			},
			"archive_path": {
				"type": "string"
			},
			"source_type": {
				"type": "string"
			}
		},
		"annotations": []
	}
}