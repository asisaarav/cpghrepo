{
	"name": "pl_master",
	"properties": {
		"activities": [
			{
				"name": "extract_to_raw",
				"type": "Switch",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"on": {
						"value": "@if(equals(pipeline().parameters.source_type, 'adls'), 'Default', 'Other')\r\n",
						"type": "Expression"
					},
					"cases": [
						{
							"value": "Other",
							"activities": [
								{
									"name": "Wait",
									"type": "Wait",
									"dependsOn": [],
									"userProperties": [],
									"typeProperties": {
										"waitTimeInSeconds": 1
									}
								}
							]
						}
					],
					"defaultActivities": [
						{
							"name": "execute_pl_child",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "pl_adls_child",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"container_name": {
										"value": "@pipeline().parameters.container_name",
										"type": "Expression"
									},
									"landing_path": {
										"value": "@pipeline().parameters.landing_path",
										"type": "Expression"
									},
									"raw_path": {
										"value": "@pipeline().parameters.raw_path",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			},
			{
				"name": "If_load_to_bronze",
				"type": "IfCondition",
				"dependsOn": [
					{
						"activity": "extract_to_raw",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@equals(pipeline().parameters.brz_load,'y' )",
						"type": "Expression"
					},
					"ifTrueActivities": [
						{
							"name": "brz_nb",
							"type": "DatabricksNotebook",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"notebookPath": {
									"value": "@pipeline().parameters.brz_nb_path",
									"type": "Expression"
								},
								"baseParameters": {
									"brz_nb_path": {
										"value": "@pipeline().parameters.brz_nb_path",
										"type": "Expression"
									}
								}
							},
							"linkedServiceName": {
								"referenceName": "ls_databricks",
								"type": "LinkedServiceReference"
							}
						}
					]
				}
			},
			{
				"name": "If_load_to_silver",
				"type": "IfCondition",
				"dependsOn": [
					{
						"activity": "If_load_to_bronze",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@equals(pipeline().parameters.slv_load,'y' )",
						"type": "Expression"
					},
					"ifTrueActivities": [
						{
							"name": "slv_nb",
							"type": "DatabricksNotebook",
							"state": "Inactive",
							"onInactiveMarkAs": "Succeeded",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"notebookPath": {
									"value": "@pipeline().parameters.slv_nb_path",
									"type": "Expression"
								}
							},
							"linkedServiceName": {
								"referenceName": "ls_databricks",
								"type": "LinkedServiceReference"
							}
						}
					]
				}
			}
		],
		"parameters": {
			"landing_path": {
				"type": "string",
				"defaultValue": "landing"
			},
			"container_name": {
				"type": "string",
				"defaultValue": "cpghvv"
			},
			"segment_grp": {
				"type": "string"
			},
			"file_name": {
				"type": "string"
			},
			"brz_nb_path": {
				"type": "string",
				"defaultValue": "/Workspace/Shared/generic_raw_to_bronze"
			},
			"slv_nb_path": {
				"type": "string"
			},
			"archive_path": {
				"type": "string"
			},
			"source_type": {
				"type": "string",
				"defaultValue": "adls"
			},
			"brz_load": {
				"type": "String"
			},
			"slv_load": {
				"type": "String"
			},
			"raw_path": {
				"type": "string",
				"defaultValue": "raw"
			},
			"raw_file_name": {
				"type": "string"
			},
			"directory": {
				"type": "string"
			},
			"load_type": {
				"type": "string"
			}
		},
		"annotations": []
	}
}