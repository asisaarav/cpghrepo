{
	"name": "pl_medallion",
	"properties": {
		"activities": [
			{
				"name": "for_each_dataset",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.list_of_paths",
						"type": "Expression"
					},
					"isSequential": false,
					"batchCount": 3,
					"activities": [
						{
							"name": "Execute Pipeline1",
							"type": "ExecutePipeline",
							"dependsOn": [
								{
									"activity": "set_filename",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "pl_master",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"directory": {
										"value": "@variables('directory')",
										"type": "Expression"
									}
								}
							}
						},
						{
							"name": "set_directory_name",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "filename",
								"value": {
									"value": "@split(item(),'/')[0]",
									"type": "Expression"
								}
							}
						},
						{
							"name": "set_filename",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "set_directory_name",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "directory",
								"value": {
									"value": "@split(item(),'/')[1]",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"list_of_paths": {
				"type": "array",
				"defaultValue": [
					"audit",
					"booking",
					"consent_management",
					"customer",
					"feedback",
					"interactions",
					"transactional"
				]
			}
		},
		"variables": {
			"filename": {
				"type": "String"
			},
			"directory": {
				"type": "String"
			}
		},
		"annotations": []
	}
}